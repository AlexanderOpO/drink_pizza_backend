{
  "name": "drink-pizza-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "better-npm-run start:dev",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "start:dev": {
      "command": "nodemon ./bin/www",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "start:prod": {
      "command": "node ./bin/www",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8080
      }
    }
  },
  "dependencies": {
    "better-npm-run": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "crypto-xor": "^1.1.0",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "hbs": "^4.1.1",
    "mongoose": "^5.9.16",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.3"
  }
}
